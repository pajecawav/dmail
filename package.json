{
	"name": "dmail",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"build-extension": "vite build --emptyOutDir",
		"start": "next start",
		"prestart": "prisma migrate deploy",
		"start-extension": "web-ext run -s build-extension",
		"bundle-extension": "web-ext build -s build-extension",
		"generate": "yarn prisma generate",
		"lint": "next lint",
		"tsc": "tsc --noEmit",
		"format": "prettier 'src/**/*.{css,js,jsx,ts,tsx}' --write",
		"format:list": "prettier --list-different 'src/**/*.{js,ts,jsx,tsx,css}'",
		"prepare": "husky install"
	},
	"dependencies": {
		"@heroicons/react": "^1.0.6",
		"@prisma/client": "3.12.0",
		"dayjs": "^1.11.0",
		"formidable": "^2.0.1",
		"next": "12.1.4",
		"react": "18.0.0",
		"react-dom": "18.0.0"
	},
	"devDependencies": {
		"@types/formidable": "^2.0.4",
		"@types/node": "^17.0.23",
		"@types/react": "17.0.43",
		"@types/react-dom": "17.0.14",
		"@vitejs/plugin-react": "^1.3.1",
		"autoprefixer": "^10.4.4",
		"eslint": "8.12.0",
		"eslint-config-next": "12.1.4",
		"husky": "^7.0.4",
		"postcss": "^8.4.12",
		"prettier": "^2.6.2",
		"prisma": "^3.12.0",
		"rollup-plugin-copy": "^3.4.0",
		"tailwindcss": "^3.0.23",
		"typescript": "4.6.3",
		"vite": "^2.9.13",
		"web-ext": "^6.8.0"
	}
}
